    "@types/puppeteer-core": "^1.9.0",
    "@types/uuid": "^3.4.5",
    "async-retry": "^1.2.3",
    "get-port-cli": "^2.0.0",
    "node-fetch": "^2.6.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "documentation": "^12.1.1",
    "electron": "^6.0.0",
    "eslint": "^6.1.0",
